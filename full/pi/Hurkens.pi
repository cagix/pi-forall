-- from Jonathan Chan
-- NOTE: displacement is not necessary for this example
module Hurkens where

data Void : Type where {}
neg : Type -> Type 
neg = \X. X -> Void
P : Type -> Type
P = \S. S -> Type
U : Type @ 1
U = (x : Type @ 0) -> ((P (P x)) -> x) -> P (P x)
tau : (P (P U)) -> U
tau = \t. \x. \f. \p. t (\s. p (f (s x f)))
sigma : U -> P (P U) @ 2
sigma = \s. s U (\t. tau t)
Delta : P U @ 2
Delta = \y. neg ((p : P U @ 1) -> sigma y p -> p (tau (sigma y)))
Omega : U @ 3
Omega = tau (\p. (x : U @ 2) -> sigma x p -> p x)
M : (x : U @ 2) -> sigma x Delta -> Delta x @ 4
M = \x. \two. \three. three Delta two (\p. three (\y. p (tau (sigma y))))

